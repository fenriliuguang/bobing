#background{
    z-index: -1;
    position:fixed; 
    background-size:100% 100%;
}

#back{
    position: absolute;
    right: 30rpx;
    top: 60rpx;
    width: 70rpx;
    height: 70rpx;
}

#userHistory {
    font-family: '汉仪尚魏手书';
    color: #F7F727;
    font-size: 30px;
    position: absolute;
    width: 400rpx;
    height: 800rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    left: calc(50% - 200rpx);
    top: calc(50% - 400rpx);
}

#userHistory text{
    display: block;
    width: 400rpx;
    text-align: center;
}